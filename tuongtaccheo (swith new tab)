from selenium import webdriver
from selenium.webdriver.common.keys import Keys
from time import sleep

browser = webdriver.Chrome(executable_path=r"C:\Users\Nguyen Dinh Khue\Documents\Lập Trình Python\new chrome\chromedriver.exe")
browser.get("https://tuongtaccheo.com")
sleep(3)

# điền thông tin đăng nhập
browser.find_element_by_xpath("/html/body/div[1]/div/div[2]/div/div/div[3]/div/button").click()
browser.find_element_by_id("name").send_keys("khuetl")
browser.find_element_by_id("password").send_keys("10102001")
sleep(1)
browser.find_element_by_name("submit").click()

# lấy nhiệm vụ follow
sleep(1)
browser.get("https://tuongtaccheo.com/tiktok/kiemtien/subcheo/")
sleep(5)
browser.find_element_by_xpath("/html/body/div[1]/div/div[3]/div/div[1]/div/div[1]/div/div/button").click()
sleep(2)

# chuyển sang tab số 2 và lấy url
browser.switch_to.window(browser.window_handles[1])
print(browser.current_url)


browser.quit()

